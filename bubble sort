#include<stdio.h>
int main()
{
	int A[50],i,j,temp,n;
	printf("/n enter the max number of elements:/n");
	scanf("%d,&n");
	printf("/n enter the elements /n");
	for(i=0;i<n;i++){
	}
	for(j=0;j<n-1-i;j++)
	{
		if(A[j]>A[j+1])
		{
			temp=A[j];
			A[j]=A[j+1];
			A[j+1]=temp;
			{
				for(i=0;i<n;i++)
				{
					printf("%d/t",A[i]);
					return 0;
				}
			}
		}
	}
}
